#include <stdio.h>
#include <stdlib.h>

int obeb(int sayi1,int sayi2)
{
	
	if(sayi1==0)
	{
		return sayi2;
	}
	else if(sayi2==0)
	{
		return sayi1;
	}
	else
	{
		return obeb(sayi2,sayi1%sayi2);
	}
	
	
	
}


     int main(){
     	
     	int sayi1,sayi2;
     	
     	printf("Obebi Alinacak 2 sayiyi Giriniz\n");
     	scanf("%d%d",&sayi1,&sayi2);
     	
     	printf("OBEB(%d,%d)=%d",sayi1,sayi2,obeb(sayi1,sayi2));
     	
     	return 0;
	 }
